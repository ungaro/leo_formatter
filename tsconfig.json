{
	"compilerOptions": {
	  "module": "NodeNext",
	  "moduleResolution": "NodeNext",
	  "target": "ES2022",
	  "lib": ["ES2022"],
	  "sourceMap": true,
	  "rootDir": "src",
	  "outDir": "dist",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", ".vscode-test"]
  }